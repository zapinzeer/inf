import pandas as pd

troskovi = [
    ["–°—Ç–∞–Ω–∞—Ä–∏–Ω–∞", 8251, 8436, 8524, 8388, 8241, 8196, 8004, 7996, 7991, 8015, 8353, 8456],
    ["–°—Ç—Ä—É—ò–∞", 4321, 4530, 4115, 3990, 3985, 3726, 3351, 3289, 3295, 3485, 3826, 3834],
    ["–¢–µ–ª–µ—Ñ–æ–Ω (—Ñ–∏–∫—Å–Ω–∏)", 1425, 1538, 1623, 1489, 1521, 1485, 1491, 1399, 1467, 1531, 1410, 1385],
    ["–¢–µ–ª–µ—Ñ–æ–Ω (–º–æ–±–∏–ª–Ω–∏)", 2181, 2235, 2073, 1951, 1989, 1945, 3017, 2638, 2171, 1831, 1926, 1833],
    ["–¢–í –∏ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç", 2399, 2399, 2399, 2399, 2399, 2399, 2399, 2399, 2399, 2399, 2399, 2399],
    ["–ü—Ä–µ–≤–æ–∑", 1830, 1830, 1830, 1830, 1950, 1950, 1450, 1450, 1950, 1950, 2050, 2050],
    ["–•—Ä–∞–Ω–∞", 23250, 23780, 24019, 24117, 24389, 24571, 24736, 24951, 25111, 25389, 25531, 25923],
    ["–û—Å—Ç–∞–ª–æ", 4500, 3700, 5100, 3500, 2750, 4250, 7320, 8250, 3270, 4290, 3200, 8390]
]

# dataframe üëÖüëÖüëÖüëÖüëÖüëÖ
stavke = [row[0] for row in troskovi]
podaci = [row[1:] for row in troskovi]
meseci = ['–à–∞–Ω', '–§–µ–±', '–ú–∞—Ä', '–ê–ø—Ä', '–ú–∞—ò', '–à—É–Ω', '–à—É–ª', '–ê–≤–≥', '–°–µ–ø', '–û–∫—Ç', '–ù–æ–≤', '–î–µ—Ü']
df = pd.DataFrame(podaci, index=stavke, columns=meseci)
# na desno vidices troskovi.csv i to otvori i deletuj ovaj comment posle toga
df.loc['–£–∫—É–ø–Ω–æ'] = df.sum(axis=0)

